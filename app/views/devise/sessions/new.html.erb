<div class="row">
  <div class="col-sm-8 col-sm-offset-2">
  <br>
    <h2 class="form_group">ログイン</h2>
  <br>
  <br>
    <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
      <div class="form-group container1">
        <%= f.label :email, "電子メール" %><br />
        <%= f.email_field :email, autofocus: true, class: "form-control form-control1" %>
      </div>

      <div class="form-group container1">
        <%= f.label :password, "パスワード" %><br />
        <%= f.password_field :password, autocomplete: "off", class: "form-control form-control1" %>
      </div>

      <div class="form-group container1">
        <%= f.submit "ログイン", class: 'btn width3' %>
      </div>
    <% end %>

    <%# <%= render "devise/shared/links" %>
  </div>
</div>